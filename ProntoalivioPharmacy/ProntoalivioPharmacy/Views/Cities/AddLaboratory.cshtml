@model ProntoalivioPharmacy.Models.LaboratoryViewModel

@{
    ViewData["Title"] = "Create";
}

<h1>Crear</h1>

<h4>Laboratorio/Farmacia</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="AddLaboratory">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="NeighborhoodId" />
            <partial name="_Laboratory" />

            <div class="form-group mt-2">
                <input type="submit" value="Crear" class="btn btn-outline-primary" />
                <a asp-action="DetailsNeighborhood" asp-route-id="@Model.NeighborhoodId" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

